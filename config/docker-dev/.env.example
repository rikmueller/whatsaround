# WhatsAround Docker Development Configuration
# Copy this file to .env for Docker development with hot reload

# Project Settings
WA_PROJECT_NAME=DockerDev
WA_TIMEZONE=UTC

# Search Parameters
WA_RADIUS_KM=5
WA_PRESETS=
WA_SEARCH_INCLUDE=
WA_SEARCH_EXCLUDE=

# Hostname (for Vite HMR - use your machine's IP for remote access)
# Example: 192.168.1.100 or localhost
WA_HOSTNAME=localhost

# Seline tracking (optional)
WA_SELINE_ENABLED=false
WA_SELINE_TOKEN=
